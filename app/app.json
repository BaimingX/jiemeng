{
  "expo": {
    "name": "Oneiro AI",
    "slug": "oneiro-ai",
    "scheme": "oneiro",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0B0C15"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.oneiro.ai"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0B0C15"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.oneiro.ai"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ]
  }
}